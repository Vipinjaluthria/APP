<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.profileinformation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/taxi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/profile"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChatActivity"/>
        <activity android:name=".Verification"/>
        <activity android:name=".phonenumber"/>
        <activity android:name=".Delete" />
        <activity android:name=".Updatebookings" />
        <activity android:name=".Searchbookings" />
        <activity
            android:name=".addbooking"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".profile" />
        <activity
            android:name=".firstactivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".googlesignin"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>